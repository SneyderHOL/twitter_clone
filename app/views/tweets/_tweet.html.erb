<% obj.each do |tweet| %>
  <div>
    <div>
      <small><strong><%= tweet.user.fullname %>, </strong></small>
      <small><%= "@#{ tweet.user.username } - " %></small>
      <small><%= tweet.created_at %></small>
    </div>
    <div>
      <%= simple_format(tweet.message) %>
    </div>
  </div>
<% end %>